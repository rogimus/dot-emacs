nums1 = [1,2,3,0,0,0]
nums2 = [2,5,6]

for i in range(len(nums2)):
    nums1.pop()

m = 3
n = 3
    
i = j = 0
nums3 = []
    
while i<m and j<n:
    if nums1[i] < nums2[j]:
        nums3.append(nums1[i])
        i += 1
    else:
        nums3.append(nums2[j])
        j += 1

if j < n:
    nums3 = nums3 + nums2[j:n]
elif i < m:
    nums3 = nums3 + nums1[i:m]
nums1 = nums3

print(nums1)