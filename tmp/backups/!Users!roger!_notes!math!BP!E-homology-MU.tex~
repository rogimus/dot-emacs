\begin{document}
\author{Roger Murray}
\title{$E_*(\MU)$ and $E_*(\bbC P^\infty)$}

\maketitle

\section{The result}

\begin{proposition}
  Let $E$ be an oriented ring spectrum. Then
  \begin{enumerate}[{\normalfont(i)}]
  \item $E_*(\bbC P^\infty)\cong \Hom_{\pi_*E}(E^*(\bbC P^\infty),\pi_*E)$
  \item $E_*(\MU)\cong \Hom_{\pi_*E}(E^*(\MU),\pi_*E)$
  \end{enumerate}
\end{proposition}

\end{document}