matrix = [[1,3,5,7],[10,11,16,20],[23,30,34,60]]
target = 3

for i in len(matrix):
    if target >=  matrix[i][0] and target <= matrix[i][-1]:
        row = i
        break

n = len(matrix[0])
mat_r = matrix[row]
while n > 1:
    if mat_r[int(n/2)] >= target:
        if mat_r[(int(n/2)] == target:
            return true
        else:
            mat_r = mat_r[int(n/2):]
    else:
        mat_r = mat_r[:int(n/2)]
    n = len(mat_r)

if target in mat_r:
    return True
return False
    
